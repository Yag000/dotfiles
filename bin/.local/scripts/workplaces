#!/usr/bin/bash


workplaces_dir=~/dotfiles/ressources/workplaces.txt

if [[ $# -eq 1 ]]; then
 find ~/Documents/l2_math-info ~/Documents/Archivos/Programas/projects ~/dotfiles -name .git -exec dirname {} \; -prune > $workplaces_dir
else
    cat $workplaces_dir
    find /home/yago/Documents/Archivos/Programas/projects -mindepth 1 -maxdepth 1 -type d 
fi

